{
    "plugins": [
        "richquotes",
        "anchors",
        "codeblock-label",
        "bootstrap-callout",
        "multipart",
        "collapsible-chapters",
        "localized-footer",
        "editlink",
        "scrollablecode",
        "book-summary-scroll-position-saver",
        "tags",
        "wide-page"
    ],
    "pluginsConfig": {
        "editlink": {
            "base": "https://github.com/marrow/mongo/tree/book",
            "label": "Edit This Page",
            "multilingual": false
        },
        "versions": {
            "type": "tags",
            "includeFilepath": true
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": []
        },
        "fontsettings": {
            "theme": "night",
            "family": "serif",
            "size": 2
        }
    }
}